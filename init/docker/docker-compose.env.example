#influxdb
INFLUXDB_HTTP_AUTH_ENABLED=true
INFLUXDB_ADMIN_USER=unpoller
INFLUXDB_ADMIN_PASSWORD=CHANGEME
INFLUXDB_DB=unpoller
INFLUXDB_ORG=unpoller
INFLUXDB_BUCKET=unpoller
INFLUXDB_ADMIN_TOKEN=unpollersecret

#grafana
GRAFANA_USERNAME=admin
GRAFANA_PASSWORD=grafanaadmin

#unifi-poller
POLLER_TAG=latest
POLLER_DEBUG=false
POLLER_SAVE_DPI=false
# API Key Authentication (RECOMMENDED)
# Generate an API key in the UniFi controller UI:
# Settings > Admins > Create New Admin > API Only (no Local Access)
UNIFI_API_KEY=your-api-key-here
# Legacy username/password authentication (comment out UNIFI_API_KEY to use):
# UNIFI_USER=unpoller
# UNIFI_PASS=set_this_on_your_controller
UNIFI_URL=https://127.0.0.1:8443
